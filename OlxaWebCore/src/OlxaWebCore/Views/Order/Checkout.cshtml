@*<div class="wrapper">*@
    <div class="container">
        <div>Тут название того,что заказал</div>
        <div>Тут доп услуги</div>
        <div>Форма с заякой как у нукли Checkout </div>
        <div class="checkout-wrap">
            <form method="post" class="checkout-form" action="" role="form">
                <div class="form-wrap">
                    <div class="inputs">
                        <div class="inputs-row-two">
                            <div class="inputs-column-one">
                                <input type="text" placeholder="Имя" required />
                                <input type="email" placeholder="Email" />
                                <input type="text" placeholder="Что-то" />
                            </div>
                            <div class="inputs-column-two">
                                <input type="text" placeholder="Фамилия" />
                                <input type="text" placeholder="Телефон" />
                                <input type="text" placeholder="Что-то" />
                            </div>
                        </div>
                        <div class="inputs-row-one">
                            <input type="text" class="inputs-long" placeholder="Название сайта/компании" />
                            <!--<div class="file">
                                <p>
                                    Если у вас есть готовый бриф или ТЗ можете приложить их к письму
                                </p>

                                <div class="file_upload">
                                    <button type="button">Выбрать</button>
                                    <div>Файл не выбран</div>
                                    <input type="file">
                                </div>
                            </div>-->
                            <div class="inputs-row-two">
                                <div class="inputs-column-one">
                                    <input type="text" placeholder="Что-то" />
                                    <input type="text" placeholder="Что-то" />
                                </div>
                                <div class="inputs-column-two">
                                    <input type="text" placeholder="Что-то" />
                                    <input type="text" placeholder="Что-то" />
                                </div>
                            </div>
                        </div>
                    </div> <!--inputs-->
                    <aside class="sidebar">
                        <div class="details">
                            <div class="subtotal">
                                <span>Промежуточный итог</span>
                                <div class="subtotal-price">25 000</div>
                            </div>
                            <div class="panel-group" id="accordion">
                                <div class="panel">
                                    <div class="panel-heading">
                                        <a class="panel-title collapsed" data-toggle="collapse" data-parent="#accordion" href="#collapseOne">
                                            Delivery
                                        </a>
                                    </div>
                                    <div id="collapseOne" class="panel-collapse collapse">
                                        <div class="panel-body">
                                            <select class="form-control" name="co_delivery">
                                                <option value="local">Local delivery</option>
                                                <option value="international">International delivery</option>
                                            </select>
                                        </div>
                                    </div>
                                </div><!-- .panel -->
                                <div class="panel">
                                    <div class="panel-heading">
                                        <a class="panel-title collapsed" data-toggle="collapse" data-parent="#accordion" href="#collapseTwo">
                                            Coupon Code
                                        </a>
                                    </div>
                                    <div id="collapseTwo" class="panel-collapse collapse" role="tabpanel">
                                        <div class="panel-body">
                                            <input type="text" name="co_coupon" class="form-control" placeholder="Enter code here">
                                        </div>
                                    </div>
                                </div><!-- .panel -->
                                <div class="panel">
                                    <div class="panel-heading">
                                        <a class="panel-title collapsed" data-toggle="collapse" data-parent="#accordion" href="#collapseThree">
                                            Payment
                                        </a>
                                    </div>
                                    <div id="collapseThree" class="panel-collapse collapse">
                                        <div class="panel-body">
                                            <div class="form-group">
                                                <label class="radio">
                                                    <input type="radio" name="co_payment" checked> PayPal
                                                </label>
                                                <label class="radio">
                                                    <input type="radio" name="co_payment"> Wire transfer
                                                </label>
                                                <label class="radio">
                                                    <input type="radio" name="co_payment"> Credit card
                                                </label>
                                            </div>
                                        </div>
                                    </div>
                                </div><!-- .panel -->
                            </div><!-- .panel-group -->
                            <div class="total">
                                <div class="text-default">Итог</div>
                                <div class="text-right"><strong>$1724</strong></div>
                            </div>
                            <button type="submit" class="btn btn-primary btn-block waves-effect waves-light">
                                Checkout
                            </button>
                        </div> <!--details-->
                    </aside><!-- .sidebar -->
                </div> <!--form-wrap-->
            </form>
        </div>
    </div> <!--container-->

@*</div>*@
